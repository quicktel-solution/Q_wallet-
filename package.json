{
    "name": "node-with-linting",
    "version": "1.0.0",
    "description": "",
    "main": "src/index.js",
    "scripts": {
        "// ------ DEVELOPMENT ----- //": "",
        "dev": "nodemon src/index.ts",
        "convert": "ts-jest config:migrate ./jest.config.js",
        "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js",
        "db:migrate": "sequelize db:migrate",
        "db:migrate:undo": "sequelize db:migrate:undo",
        "db:drop": "sequelize db:drop",
        "db:create": "sequelize db:create",
        "// ------ TESTING ----- //": "",
        "test": "jest --watch",
        "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
        "// ------ BUILDING ----- //": "",
        "build": "tsc",
        "build:watch": "tsc -w",
        "// ------ LINTING ----- //": "",
        "lint:list": "eslint */**/*.ts --fix",
        "lint": "eslint */**/*.ts --fix",
        "// ------ PRODUCTION ----- //": "",
        "start:prod": "nodemon src/index.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@graphql-tools/load-files": "^6.2.4",
        "@graphql-tools/merge": "^6.2.4",
        "@types/bcrypt": "^3.0.0",
        "@types/cors": "^2.8.7",
        "@types/jest": "^24.0.12",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/multer": "^1.4.4",
        "apollo-server-express": "^2.17.0",
        "bcrypt": "^5.0.0",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "graphql-middleware": "^4.0.2",
        "graphql-shield": "^7.3.7",
        "graphql-type-json": "^0.3.2",
        "jsonwebtoken": "^8.5.1",
        "multer": "^1.4.2",
        "mysql": "^2.18.1",
        "mysql2": "^2.1.0",
        "sequelize-msnodesqlv8": "^0.2.6-beta.8",
        "sequelize-typescript": "^1.1.0",
        "sqlite3": "^5.0.0",
        "ts-jest": "^24.0.0",
        "twilio": "^3.55.0"
    },
    "devDependencies": {
        "@babel/polyfill": "^7.2.5",
        "@types/body-parser": "^1.19.0",
        "@types/express": "^4.17.8",
        "@types/node": "^11.15.32",
        "@types/nodemailer": "^6.4.0",
        "@typescript-eslint/eslint-plugin": "^1.4.2",
        "@typescript-eslint/parser": "^1.4.2",
        "eslint": "^5.15.1",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "jest": "^24.3.0",
        "nodemon": "^1.19.4",
        "prettier": "^1.16.4",
        "sequelize": "^6.5.0",
        "sequelize-cli": "^6.2.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    }
}
